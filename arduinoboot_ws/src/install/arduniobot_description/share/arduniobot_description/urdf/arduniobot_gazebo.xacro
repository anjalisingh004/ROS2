<?xml version="1.0"?>

<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name ="arduniobot">

    <gazebo>
        <plugin filename="libgazebo_ros2_control.so" name="gazebo_ros2_control">
            <robot_param>robot_description</robot_param>
            <robot_param_node>robot_state_publisher</robot_param_node>
            <parameters>$(find arduniobot_controller)/config/arduniobot_controller.yaml</parameters>
        </plugin>
    </gazebo>

</robot>
